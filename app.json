{
  "expo": {
    "name": "recipe-finder-meal-planner",
    "slug": "recipe-finder-meal-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "recipefindermealplanner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-BlackItalic.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Medium.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Rubik",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Black.ttf",
                    "weight": 900,
                    "style": "black"
                  },
                  {
                    "path": "./assets/fonts/Rubik-BlackItalic.ttf",
                    "weight": 900,
                    "style": "black-italic"
                  },
                  {
                    "path": "./assets/fonts/Rubik-Light.ttf",
                    "weight": 300,
                    "style": "light"
                  },
                  {
                    "path": "./assets/fonts/Rubik-Medium.ttf",
                    "weight": 500,
                    "style": "medium"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Rubik-BlackItalic.ttf",
              "./assets/fonts/Rubik-Light.ttf",
              "./assets/fonts/Rubik-Medium.ttf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
